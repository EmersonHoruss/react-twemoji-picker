{
  "name": "react-twemoji-picker",
  "version": "1.0.0",
  "description": "A simple emoji picker for React based on Twemoji",
  "main": "dist/react-twemoji-picker.cjs.js",
  "module": "dist/react-twemoji-picker.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:types",
    "build:types": "tsc --project tsconfig.types.json",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "lodash.pick": "^4.4.0",
    "react": "^18.0.0",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "^18.0.0",
    "twemoji": "^14.0.2",
    "twitter-text": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "react",
    "emoji",
    "twemoji",
    "picker",
    "library"
  ],
  "author": "Emerson Perales",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmersonHoruss/react-twemoji-picker"
  }
}
